<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="TitleBar">
        <attr name="tb_title" format="string" />
        <attr name="tb_title_color" format="color" />
        <attr name="tb_max_title_length" format="integer" />
        <attr name="tb_bg_color" format="color" />
        <attr name="tb_bg_res" format="reference" />
        <attr name="tb_default_back" format="boolean" />
        <attr name="tb_left_src" format="reference" />
        <attr name="tb_right_src" format="reference" />
        <attr name="tb_right_src_b" format="reference" />
        <attr name="tb_left_text" format="string" />
        <attr name="tb_right_text" format="string" />
        <attr name="tb_right_text_color" format="color" />
        <attr name="tb_define_id" format="boolean" />
        <attr name="tb_no_line" format="boolean" />
        <attr name="tb_go_back" format="boolean" />
    </declare-styleable>

    <declare-styleable name="MLoadingDialog">
        <attr name="mld_tip" format="string" />
        <attr name="mld_load_bg" format="color" />
        <attr name="mld_defined_id" format="boolean" />
        <attr name="mld_no_shape" format="boolean" />
        <attr name="mld_txt_color" format="color" />
    </declare-styleable>

    <declare-styleable name="CircularImageView">
        <attr name="border" format="boolean" />
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_shadow" format="boolean" />
        <attr name="civ_shadow_color" format="color" />
        <attr name="civ_shadow_radius" format="float" />
    </declare-styleable>

    <declare-styleable name="PxBlurringView">
        <attr name="pbv_blur_radius" format="integer" />
        <attr name="pbv_down_sample_factor" format="integer" />
        <attr name="pbv_overlay_color" format="color" />
    </declare-styleable>

    <declare-styleable name="RoundImageView">
        <attr name="type" format="enum">
            <enum name="circle" value="1" />
            <enum name="round" value="2" />
        </attr>
        <attr name="radius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="MCropCoverView">
        <attr name="ccv_ratio_type" format="enum">
            <enum name="Square" value="1" />
            <enum name="Rectangle" value="2" />
        </attr>
        <attr name="ccv_left_dis" format="integer" />
        <attr name="ccv_size" format="integer" />
        <attr name="ccv_crop_width" format="integer" />
        <attr name="ccv_crop_height" format="integer" />
    </declare-styleable>

    <declare-styleable name="SwipeMenuLayout">
        <attr name="sml_swipe_enable" format="boolean" />
        <attr name="sml_ios" format="boolean" />
        <attr name="sml_left_swipe" format="boolean" />
        <attr name="sml_top" format="dimension" />
        <attr name="sml_bot" format="dimension" />
    </declare-styleable>

    <declare-styleable name="MCheckBox">
        <attr name="mcb_drawable" format="reference" /><!--小图标背景，设置为selector-->
        <attr name="mcb_drawable_width" format="dimension" /><!--小图标宽度，缩放需要-->
        <attr name="mcb_drawable_height" format="dimension" /><!--小图标高度，缩放需要-->
        <attr name="mcb_text" format="string" />
        <attr name="mcb_text_focus" format="string" />
        <attr name="mcb_txt_color" format="color" />
        <attr name="mcb_txt_color_focus" format="color" />
        <attr name="mcb_checked" format="boolean" />
        <attr name="mcb_self_control" format="boolean" />
        <attr name="mcb_no_bg" format="boolean" />
    </declare-styleable>

    <declare-styleable name="PageIndicatorView">
        <attr name="piv_orientation">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>

        <attr name="piv_viewPager" format="reference" />
        <attr name="piv_select" format="integer" />

        <attr name="piv_count" format="integer" />
        <attr name="piv_dynamicCount" format="boolean" />

        <attr name="piv_radius" format="dimension" />
        <attr name="piv_padding" format="dimension" />
        <attr name="piv_strokeWidth" format="dimension" />
        <attr name="piv_scaleFactor" format="float" />
        <attr name="piv_autoVisibility" format="boolean" />

        <attr name="piv_unselectedColor" format="color" />
        <attr name="piv_selectedColor" format="color" />

        <attr name="piv_interactiveAnimation" format="boolean" />
        <attr name="piv_animationDuration" format="integer" />
        <attr name="piv_animationType">
            <enum name="none" value="0" />
            <enum name="color" value="1" />
            <enum name="scale" value="2" />
            <enum name="worm" value="3" />
            <enum name="slide" value="4" />
            <enum name="fill" value="5" />
            <enum name="thinWorm" value="6" />
            <enum name="drop" value="7" />
            <enum name="swap" value="8" />
            <enum name="scale_down" value="9" />
        </attr>
        <attr name="piv_rtl_mode">
            <enum name="on" value="0" />
            <enum name="off" value="1" />
            <enum name="auto" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="FlowLayoutA">
        <attr name="fla_columns" format="integer" />
        <attr name="fla_padding" format="dimension" />
        <attr name="fla_left_padding" format="dimension" />
        <attr name="fla_top_padding" format="dimension" />
        <attr name="fla_right_padding" format="dimension" />
        <attr name="fla_bottom_padding" format="dimension" />
        <!--自动计算间距-->
        <attr name="fla_auto_padding" format="boolean" />
        <!--自动计算水平间距-->
        <attr name="fla_auto_hor_padding" format="boolean" />
        <!--自动计算垂直间距-->
        <attr name="fla_auto_ver_padding" format="boolean" />
        <attr name="fla_center_horizontal" format="boolean" />
        <attr name="fla_center_vertical" format="boolean" />
        <attr name="fla_hor_margin" format="dimension" />
        <attr name="fla_ver_margin" format="dimension" />
        <attr name="fla_min_height" format="dimension" />
        <attr name="fla_selection" format="integer" />
        <attr name="fla_max_line" format="integer" />
    </declare-styleable>

    <declare-styleable name="TextProgressBar">
        <attr name="tpb_text" format="string" />
    </declare-styleable>

    <declare-styleable name="MCheckableTextView">
        <attr name="ctv_checked" format="boolean" />
    </declare-styleable>

    <declare-styleable name="MCheckableImageView">
        <attr name="bCheck" format="boolean" />
    </declare-styleable>
</resources>